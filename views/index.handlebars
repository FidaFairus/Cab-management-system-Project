            <th>Date</th>
            <th>Time</th>
            <th>userid</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr>
            <td>{{this.dataValues.bookingId}}</td>
            <td>{{this.dataValues.pickupPoint}}</td>
            <td>{{this.dataValues.destination}}</td>
